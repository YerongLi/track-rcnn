    "import numpy as np\n",
    "\n",
    "%matplotlib inline\n",
    "import matplotlib\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "N = 150\n",
    "x = range(N)\n",
    "foo = lambda x: 1. / N * np.log(N / (N-x+1.)) * (N - x)\n",
    "y = [foo(xi) for xi in x]\n",
    "print(np.array(y) / sum(y))"
   ]
